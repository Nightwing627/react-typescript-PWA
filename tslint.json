{
  "defaultSeverity": "warning",
  "rules": {
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "eofline": true,
    "forin": false,
    "indent": [true, "spaces", "tabs"],
    "label-position": true,
    "max-line-length": [true, 500],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "log", "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-consecutive-blank-lines": true,
    "no-switch-case-fall-through": false,
    "trailing-comma": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "defaultSeverity": "warning",
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "radix": false,
    "semicolon": true,
    "triple-equals": [true, "allow-null-check"],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "quotemark": [true, "double", "jsx-double"]
  }
}
